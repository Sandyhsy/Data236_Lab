# Server
PORT=3000
NODE_ENV=production
SESSION_SECRET=please_change_me

# Databases
MONGODB_URI=mongodb://mongodb-service:27017/airbnb_clone
MONGO_URL=mongodb://mongodb-service:27017/airbnb_clone
MONGO_DB=airbnb_clone
MYSQL_HOST=mysql-service
MYSQL_USER=airbnb_user
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=airbnb_clone

# Kafka
KAFKA_BROKERS=kafka-service:9092

# CORS
ALLOW_ALL_CORS=true
CLIENT_ORIGIN=http://your-frontend-url

# S3 uploads
AWS_REGION=us-east-1
S3_BUCKET=lab1-host-images
S3_PUBLIC_BASE=https://lab1-host-images.s3.us-east-2.amazonaws.com
S3_ACCELERATE=0

# AI service (backend -> agent)
AGENT_URL=http://ai-service:8000/ai

# Optional APIs
PEXELS_API_KEY=your_pexels_api_key
